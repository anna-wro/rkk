{
  "name": "rkk",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest && npm run check-formatting && npm run lint",
    "test:watch": "jest --watch",
    "check-formatting": "prettier --check './src/**/*.{ts,tsx}'",
    "fix-formatting": "prettier --write './src/**/*.{ts,tsx}'",
    "lint": "next lint",
    "prepare": "husky install"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "gray-matter": "^4.0.3",
    "luxon": "^2.5.2",
    "next": "13.5.0",
    "next-mdx-remote": "^3.0.4",
    "next-pwa": "^5.3.1",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.5.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "@types/luxon": "^2.0.3",
    "@types/node": "^16.9.1",
    "@types/react": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "autoprefixer": "^10.3.4",
    "babel-jest": "^27.2.0",
    "cors": "^2.8.5",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.0",
    "jest": "^27.2.0",
    "lint-staged": "^11.1.2",
    "postcss": "^8.4.31",
    "prettier": "^2.4.0",
    "pwa-asset-generator": "^4.3.6",
    "react-screen-wake-lock": "^1.1.10",
    "tailwindcss": "^2.2.15",
    "typescript": "^4.4.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix",
      "npm run fix-formatting"
    ]
  }
}
